{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "functions": {
    "api/*.py": {
      "runtime": "python3.11",
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "aimakerspace/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
} 